{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "_sdc_file_name": {
            "type": ["null","string"]
        },
        "_sdc_line_number": {
            "type": ["null","integer"]
        },
        "_sdc_last_modified": {
            "type": ["null","string"],
            "format": "date-time"
        },
        "DateTime": {
            "type": "string"
        },
        "StoreNo": {
            "type": "string"
        },
        "Traffic": {
            "type": "integer"
        },
        "StaffCount": {
            "type": "integer"
        },
        "Traffic_IN": {
            "type": ["integer","null"]
        }
    },
    "required": [
        "DateTime",
        "StoreNo",
        "Traffic",
        "StaffCount"
    ]
}